@model List<CarViewModel>
    <table>
        <tr>
            <th>Make </th>
            <th>Model </th>
            <th>Distance Travelled </th>
        </tr>
        @foreach (var car in Model)
        {
            <tr>
                <td> @car.Make </td>
                <td> @car.Model </td>
                <td> @car.TravelledDistance </td>
            </tr>
        }
    </table>
