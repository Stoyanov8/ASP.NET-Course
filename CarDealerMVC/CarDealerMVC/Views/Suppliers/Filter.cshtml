@model List<SupplierViewModel>
    <table>
        <tr>
            <th>ID </th>
            <th>Name </th>
            <th>Number of parts </th>
        </tr>
        @foreach (var supplier in Model)
        {
            <tr>
                <td> @supplier.Id </td>
                <td> @supplier.Name </td>
                <td> @supplier.PartsCount </td>
            </tr>
        }
    </table>
